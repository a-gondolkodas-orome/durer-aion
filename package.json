{
  "name": "bgio-tutorial",
  "version": "0.1.0",
  "private": false,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.16.7",
    "@mui/x-data-grid": "^7.16.0",
    "@sentry/node": "^7.58.1",
    "@sentry/react": "^7.16.0",
    "@sentry/tracing": "^7.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/koa-basic-auth": "^2.0.4",
    "@types/koa-mount": "^4.0.1",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.4.0",
    "bgio-postgres": "^1.0.13",
    "boardgame.io": "^0.50.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "formik": "^2.4.2",
    "koa-basic-auth": "^4.0.0",
    "koa-mount": "^4.0.0",
    "nanoid": "^3.0.0",
    "notistack": "^2.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.3.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-text-mask": "^5.5.0",
    "recoil": "^0.7.7",
    "swr": "^2.2.5",
    "urlcat": "^2.0.4",
    "vite": "^7.0.3",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "predeploy": "cross-env PUBLIC_URL=/durer-aion REACT_APP_WHICH_VERSION=c npm run build",
    "deploy": "gh-pages -d build",
    "lint": "turbo lint",
    "check:server": "babel-node --extensions \".ts\" --presets=@babel/preset-typescript --plugins=@babel/plugin-transform-modules-commonjs src/server.ts sanity-check",
    "start": "turbo start",
    "build": "turbo build",
    "spell-check": "cspell \"**/*.{ts,tsx}\" --no-progress",
    "dev:server": "turbo dev --filter=online-backend --filter=game --filter=schemas --filter=strategy",
    "dev:offline": "turbo dev --filter=offline-frontend --filter=game --filter=strategy --filter=common-frontend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": 0
    }
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/{client,common,games}/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "npm@8.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@sentry/types": "^7.57.0",
    "@types/pg": "^8.6.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-text-mask": "^5.4.14",
    "concurrently": "^9.2.0",
    "cspell": "^6.31.1",
    "gh-pages": "^4.0.0",
    "nodemon": "^2.0.18",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3"
  }
}
