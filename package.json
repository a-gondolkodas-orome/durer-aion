{
  "name": "bgio-tutorial",
  "version": "0.1.0",
  "private": false,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.16.7",
    "@mui/x-data-grid": "^7.16.0",
    "@sentry/node": "^7.58.1",
    "@sentry/react": "^7.16.0",
    "@sentry/tracing": "^7.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/koa-basic-auth": "^2.0.4",
    "@types/koa-mount": "^4.0.1",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "axios": "^1.4.0",
    "bgio-postgres": "^1.0.13",
    "boardgame.io": "^0.50.0-alpha.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "formik": "^2.4.2",
    "koa-basic-auth": "^4.0.0",
    "koa-mount": "^4.0.0",
    "nanoid": "^3.0.0",
    "notistack": "^2.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-text-mask": "^5.5.0",
    "recoil": "^0.7.7",
    "swr": "^2.2.5",
    "typescript": "^4.7.4",
    "urlcat": "^2.0.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "predeploy": "cross-env PUBLIC_URL=/durer-aion REACT_APP_PAGE_CONFIG=gh-pages npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src/ --ext js,ts,jsx,tsx",
    "check:server": "babel-node --extensions \".ts\" --presets=@babel/preset-typescript --plugins=@babel/plugin-transform-modules-commonjs src/server.ts sanity-check",
    "dev:server": "nodemon -e js,ts --exec babel-node --extensions \".ts\" --presets=@babel/preset-typescript --plugins=@babel/plugin-transform-modules-commonjs src/server.ts",
    "start": "cross-env REACT_APP_PAGE_CONFIG=gh-pages react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env react-scripts test . --coverage",
    "spell-check": "cspell \"**/*.{ts,tsx}\" --no-progress",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": 0
    }
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/{client,common,games}/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/types": "^7.57.0",
    "@types/pg": "^8.6.5",
    "@types/react-text-mask": "^5.4.14",
    "cspell": "^6.31.1",
    "gh-pages": "^4.0.0",
    "nodemon": "^2.0.18"
  }
}
