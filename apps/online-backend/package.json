{
  "name": "online-backend",
  "version": "0.0.0",
  
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.ts*\" --max-warnings=0",
    "build": "npx tsc",
    "start": "node dist/src/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/server.js --watch ../packages\"",
    "build_old": "babel-node --extensions \".ts\" --presets=@babel/preset-typescript --plugins=@babel/plugin-transform-modules-commonjs src/server.ts sanity-check",
    "start_old": "nodemon -e js,ts --exec babel-node --extensions \".ts\" --presets=@babel/preset-typescript --plugins=@babel/plugin-transform-modules-commonjs src/server.ts",
    "spell-check": "cspell \"**/*.{ts,tsx}\" --no-progress"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "boardgame.io": "^0.50.0-alpha.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11",
    "game": "*",
    "schemas": "*",
    "strategy": "*"
  },
  "devDependencies": {
    "cspell": "^6.31.1",
    "gh-pages": "^4.0.0",
    "nodemon": "^2.0.18",
    "turbo": "^2.5.3"
  }
}
